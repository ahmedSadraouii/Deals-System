{
  "name": "api-content",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build:api-gen": "openapi-generator-cli generate -i api-spec.json -g typescript -o generated",
    "build:tsc": "tsc -d --declarationMap",
