    "dev": "run-s build:api-gen dev:watch",
    "dev:watch": "tsc-watch -d --noClear --declarationMap"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "2.9.0",
    "@types/url-parse": "1.4.4",
    "npm-run-all": "4.1.5",
    "tsc-watch": "6.0.4",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "api": "*",
    "url-parse": "1.4.3",
    "whatwg-fetch": "3.6.2"
  }
}
