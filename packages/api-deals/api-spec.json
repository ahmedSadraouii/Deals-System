    "components": {
        "schemas": {
            "AddFavoriteInputModel": {
                "required": [
                    "dealId"
                ],
                "type": "object",
                "properties": {
                    "dealId": {
                        "type": "string",
                        "format": "uuid"
                    }
                },
                "additionalProperties": false
            },
            "CreateOrUpdateDealInputModel": {
                "required": [
                    "id",
                    "maxOrderQuantity",
                    "name",
                    "price",
                    "promotionEnd",
                    "promotionStart"
                ],
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string",
                        "description": "Gets or sets the unique identifier of the deal.",
                        "format": "uuid"
                    },
                    "name": {
                        "minLength": 1,
                        "type": "string",
                        "description": "Gets or sets the name of the deal."
                    },
                    "price": {
                        "maximum": 4294967295,
                        "minimum": 0,
                        "type": "integer",
                        "description": "Gets or sets the price of the deal.",
                        "format": "int32"
                    },
                    "promotionStart": {
                        "type": "string",
                        "description": "Gets or sets the start date of the promotion.",
                        "format": "date-time"
                    },
                    "promotionEnd": {
                        "type": "string",
                        "description": "Gets or sets the end date of the promotion.",
                        "format": "date-time"
                    },
                    "maxOrderQuantity": {
                        "type": "integer",
                        "description": "Gets or sets the maximum order quantity for the deal.",
                        "format": "int32"
                    }
                },
                "additionalProperties": false,
                "description": "Represents the input model for creating or updating a deal."
            },
            "CreateOrderInputModel": {
                "type": "object",
                "properties": {
                    "items": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/OrderItemModel"
                        },
                        "nullable": true
                    }
                },
                "additionalProperties": false
            },
            "DealModel": {
                "required": [
                    "name"
                ],
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string",
                        "description": "Gets or sets the unique identifier for the deal.",
                        "format": "uuid"
                    },
                    "name": {
                        "minLength": 1,
                        "type": "string",
                        "description": "Gets or sets the name of the deal."
                    },
                    "price": {
                        "maximum": 4294967295,
                        "minimum": 0,
                        "type": "integer",
                        "description": "Gets or sets the price of the deal.",
                        "format": "uint32"
                    },
                    "promotionStart": {
                        "type": "string",
                        "description": "Gets or sets the start date of the promotional period.",
                        "format": "date-time"
                    },
                    "promotionEnd": {
                        "type": "string",
                        "description": "Gets or sets the end date of the promotional period.",
                        "format": "date-time"
                    },
                    "maxOrderQuantity": {
                        "maximum": 65535,
                        "minimum": 1,
                        "type": "integer",
                        "description": "Gets or sets the maximum quantity that can be ordered.",
                        "format": "uint16"
                    }
                },
                "additionalProperties": false,
                "description": "Represents a deal for a promotional offer."
            },
            "DealModelPagedList": {
                "type": "object",
                "properties": {
                    "take": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "skip": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "total": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "items": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/DealModel"
                        },
                        "nullable": true
                    }
                },
                "additionalProperties": false
            },
            "FavoriteModel": {
                "type": "object",
                "properties": {
                    "dealId": {
                        "type": "string",
                        "format": "uuid"
                    },
                    "createdAt": {
                        "type": "string",
                        "format": "date-time"
                    }
                },
                "additionalProperties": false
            },
            "FavoriteModelPagedList": {
                "type": "object",
                "properties": {
                    "take": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "skip": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "total": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "items": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/FavoriteModel"
                        },
                        "nullable": true
                    }
                },
                "additionalProperties": false
            },
