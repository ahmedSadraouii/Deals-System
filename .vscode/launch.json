    "version": "0.2.0",
    "configurations": [
      {
          "type": "chrome",
          "request": "launch",
          "name": "Launch Chrome Debugger",
          "url": "http://localhost:4420",
          "webRoot": "${workspaceFolder}",
          "presentation": {
              "hidden": true
          }
      },
      {
          "command": "yarn dev",
          "name": "Debug CustomerWeb",
          "cwd": "${workspaceFolder}",
          "request": "launch",
          "type": "node-terminal",
          "serverReadyAction": {
              "action": "startDebugging",
              "name": "Launch Chrome Debugger",
              "pattern": "listening on localhost:4420"
          }
      },
    ]
  }
  